public class Main {
    public static void main(String[] args) {
        int[] arr = {5, 3, 8, 1, 4, 1};
        int min1 = Integer.MAX_VALUE, min2 = Integer.MAX_VALUE;
        for (int num : arr) {
            if (num < min1) { min2 = min1; min1 = num; }
            else if (num < min2 && num != min1) min2 = num;
        }
        System.out.println("Smallest: " + min1 + ", Second: " + min2);
    }
}
