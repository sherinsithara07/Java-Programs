public class Main {
    public static void main(String[] args) {
        String code = args[0];
        boolean isPalindrome = true;
        for (int i = 0; i < code.length() / 2; i++) {
            if (code.charAt(i) != code.charAt(code.length() - 1 - i)) {
                isPalindrome = false;
                break;
            }
        }
        System.out.println(isPalindrome ? "Valid" : "Invalid");
    }
}
